            "synapseWorkspaceName": "$(serviceShortName)$(pdg.environment.shortName)$(regionShortName)workspace"
        },
        "processingStorage": {
            "resourceGroupName": "purview-rg-internal-storage",
            "accountName": "dgprocshared$(pdg.environment.shortName)$(regionShortName)",
            "tableName": "AccountStorageDetails"
        }
