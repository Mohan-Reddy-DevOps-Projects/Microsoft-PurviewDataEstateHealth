{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "clientCertificateConfiguration": {
        "allowListedDataPlaneSubjectNames": [
            "localhost.gateway.purviewshare.azure-test.com"
        ],
        "allowListedIssuerNames": [
            "AME"
        ]
    },
    "apiServiceClientCertificates": {
        "refreshRate": "00:10:00",
        "commonKeyVaultUri": "https://dgcatalog-df-kv-wus2.vault.azure.net/"
    },
    "environment": {
        "environment": "development",
        "location": "localhost",
        "permittedApiVersions": [
            "2023-10-01-preview"
        ]
    },
    "service": {
        "apiServicePort": "5001",
        "workerServicePort": "5003",
        "apiServiceReadinessProbePort": "8081",
        "workerServiceReadinessProbePort": "8082",
        "readinessProbePath": "/healthz/ready"
    },
    "sslConfiguration": {
        "certificateName": "dataaccess-ssl-cert-final"
    },
    "genevaConfiguration": {
        "metricsAccount": "PurviewDataGovTest",
        "metricsNamespace": "ActiveGlossaryTest",
        "genevaServicesHost": "geneva-services",
        "genevaServicesPort": "8125",
        "defaultDimensions": {
            "Environment": "Dogfood",
            "RoleLocation": "westus2"
        }
    },
    "ServerCertificates": {
        "RefreshRate": "00:10:00",
        "Certificates": {
            "Default": {
                "KeyVaultUri": "https://dgcatalog-dev-kv-wus2.vault.azure.net/",
                "SecretName": "dgcatalog-server-dev-cert",
                "RefreshRate": "1.00:00:00"
            }
        }
    },
    "ClientCertificates": {
        "RefreshRate": "00:10:00",
        "Certificates": {
            "ArtifactStoreClient": {
                "KeyVaultUri": "https://dgcatalog-dev-kv-wus2.vault.azure.net/",
                "SecretName": "dgcatalog-client-dev-cert",
                "RefreshRate": "1.00:00:00"
            }
        }
    },
    "jobManagerConfiguration": {
        "backgroundJobStorageResourceId": "UseDevelopmentStorage=true"
    }
}
